{
  "name": "redux-simple-starter",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "nodemon --watch webpack.config.js --watch webpack.parts.js --exec \"npm run dev-server\"",
    "dev-server": "webpack-dev-server --env development",
    "build": "webpack --env production",
    "deploy": "npm run build && gh-pages -d build",
    "test": "npm run test:mocha && npm run test:mocha-wo-proxy",
    "test:mocha": "BABEL_ENV=test mocha --compilers js:babel-core/register --require ./test/setup.js 'test/**/*_spec.@(js|jsx)'",
    "test:mocha-wo-proxy": "npm run test:mocha -- --require ./test/setupSansProxy.js",
    "test:watch": "npm run test:mocha -- --watch --watch-extensions jsx",
    "test:lint": "eslint . --ext .js --ext .jsx --ignore-path .gitignore --cache",
    "stats": "webpack --env production --profile --json > stats.json"
  },
  "author": "Velenir <velenir@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.7.6",
    "babel-core": "^6.23.1",
