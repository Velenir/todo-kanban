{
  "plugins": [
    "syntax-dynamic-import",
    ["babel-plugin-transform-builtin-extend", {
        "globals": ["Array"]
    }]
  ],
  "presets": [
		[
      "es2015",
      {
        "modules": false
      }
    ],
    "react",
    "stage-2"
  ],
  "env": {
    "test": {
      "presets": [
        "es2015",
        "stage-2"
      ],
			"plugins": [
        "syntax-dynamic-import",
        "dynamic-import-node",
        ["babel-plugin-transform-builtin-extend", {
            "globals": ["Array"]
        }],
        ["import-redirect", {
            "redirect": {
              "\\.s?css$": false,
              "/node_modules/react-redux": "./test/connectNoop",
              "/images/.+\\.svg$": "./test/svg2react"
            }
        }]
    	]
    }
  }
}
